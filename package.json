{
  "name": "march-inventory",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "packages/**"
  ],
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "cleanup": "./cleanup.sh",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local": "export $(cat local.env | xargs) && rm -Rf dist && nest start --watch --debug",
    "start:dev": "sh delete-conflict-dependencies.sh && source .env && rm -Rf dist && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:dev": "export $(cat dev.env | xargs) && prisma migrate dev --preview-feature",
    "gql:ts:gen": "ts-node ./scripts/generate-typings.ts",
    "seed": "export $(cat dev.env | xargs) && ts-node prisma/seed/index.ts",
    "prisma:gen": "prisma generate --schema=./prisma/schema.prisma",
    "rebuild": "rm -rf node_modules && pnpm i . && pnpm prisma:gen"
  },
  "dependencies": {
    "@march/core": "workspace:1.0.0",
    "@nestjs-modules/ioredis": "^1.0.1",
    "@nestjs/axios": "0.1.0",
    "@nestjs/common": "9.0.3",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "9.0.3",
    "@nestjs/graphql": "7.11.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/microservices": "9.2.1",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.0.3",
    "@prisma/client": "3.12.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "apollo-cache-control": "^0.14.0",
    "apollo-server-core": "2.25.2",
    "apollo-server-express": "2.25.2",
    "apollo-server-types": "0.9.0",
    "axios": "1.2.1",
    "bcrypt": "^5.1.0",
    "crypto-random-string": "3.3.1",
    "express": "^4.18.2",
    "graphql": "15.5.1",
    "ioredis": "^5.2.5",
    "jsonwebtoken": "^8.5.1",
    "march-inventory": "link:",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "ts-morph": "14.0.0",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.4",
    "@types/passport-jwt": "^3.0.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "prisma": "3.12.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
